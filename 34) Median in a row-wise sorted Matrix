class Solution{   
public:
    int median(vector<vector<int>> &mat, int n, int m){
        // code here  
        
       vector<int>v; 
       
       for(int i=0;i<n;i++)
       {
           for(int j=0;j<m;j++) v.push_back(mat[i][j]);
       }
    
       
       sort(v.begin(),v.end());
       
       
        int x=v.size();
        
        return v[(x-1)/2];
       
       
        
        
    }
};
